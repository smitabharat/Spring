<?xml version = "1.0" encoding = "UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context.xsd">

	<bean id="ad" class="com.hefshine.bean.Address">
	<constructor-arg name="city" value="pune"/>
	<constructor-arg name="pin" value="413456"/>
	</bean>
	
	<bean id="date" class="com.hefshine.bean.Date">
	
	<property name="dd" value="01"/>
	<property name="mm" value="12"/>
	<property name="yy" value="62"/>
	</bean>
	
	<bean id="employee" class="com.hefshine.bean.Employee" autowire="byType">
	<property name="id" value="101"/>
	<property name="name" value="smita"/>
	
	
	</bean>

</beans>